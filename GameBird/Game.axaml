<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="1920" d:DesignHeight="1080"
        x:Class="GameBird.Game"
        Title="Game"
        WindowState="FullScreen">
    <Window.Styles>
        <StyleInclude Source="Styles.axaml"/>
    </Window.Styles>
        
    <Grid x:Name="MainGrid" Classes="screen">
        <Border x:Name="GoldSquare" Classes="bird"/>
        <Button x:Name="JumpButton" Classes="invisible_button"/>
        <StackPanel Classes="panel_count">
            <TextBlock Classes="count1"/>
            <TextBlock x:Name="TextCount" Classes="count2"/>
        </StackPanel>
    </Grid>
</Window>